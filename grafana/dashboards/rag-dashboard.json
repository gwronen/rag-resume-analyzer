{
  "dashboard": {
    "title": "RAG Application Metrics",
    "tags": ["rag", "prometheus"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Questions Per Second",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(rag_questions_total[5m])",
            "legendFormat": "{{status}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Question Duration (95th percentile)",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(rag_question_duration_seconds_bucket[5m]))",
            "legendFormat": "95th percentile"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0}
      },
      {
        "id": 3,
        "title": "Total Questions",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(rag_questions_total)",
            "legendFormat": "Total"
          }
        ],
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 8}
      },
      {
        "id": 4,
        "title": "Documents Loaded",
        "type": "stat",
        "targets": [
          {
            "expr": "rag_documents_loaded",
            "legendFormat": "Documents"
          }
        ],
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 8}
      },
      {
        "id": 5,
        "title": "Chunks Created",
        "type": "stat",
        "targets": [
          {
            "expr": "rag_chunks_created",
            "legendFormat": "Chunks"
          }
        ],
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 8}
      },
      {
        "id": 6,
        "title": "Vector Search Duration",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(rag_vector_search_duration_seconds_bucket[5m]))",
            "legendFormat": "95th percentile"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 12}
      },
      {
        "id": 7,
        "title": "Question Success Rate",
        "type": "piechart",
        "targets": [
          {
            "expr": "sum by (status) (rag_questions_total)",
            "legendFormat": "{{status}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 12}
      }
    ],
    "schemaVersion": 16,
    "version": 1
  }
}

